<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/check_box_checked" android:state_checked="true" /> <!-- pressed -->
    <item android:drawable="@drawable/check_box_checked" android:state_focused="true" /> <!-- focused -->
    <item android:drawable="@drawable/check_box_unchecked" android:state_checked="false" /> <!-- hovered -->
    <item android:drawable="@drawable/check_box_unchecked" /> <!-- default -->
</selector>